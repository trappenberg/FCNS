    "import matplotlib.pyplot as plt\n",
    "\n",
    "t = np.arange(0,10,0.1)\n",
    "v = t*np.exp(-t/2)\n",
    "plt.plot(t,v); plt.xlabel(\"t\"); plt.ylabel(\"v\")"
